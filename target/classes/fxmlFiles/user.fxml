<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="89.0" prefWidth="715.0" style="-fx-background-color: ffa9a9;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserController">
    <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="715.0" spacing="5.0">
         <children>
            <TextArea fx:id="username" prefHeight="43.0" prefWidth="84.0" promptText="username" styleClass="jfx-text-field" stylesheets="@style.css">
               <HBox.margin>
                  <Insets bottom="30.0" top="30.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextArea>
            <TextArea fx:id="nickname" prefHeight="41.0" prefWidth="91.0" promptText="nickname" styleClass="jfx-text-field" stylesheets="@style.css">
               <HBox.margin>
                  <Insets bottom="30.0" top="30.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextArea>
            <TextArea fx:id="email" prefHeight="41.0" prefWidth="169.0" promptText="email" styleClass="jfx-text-field" stylesheets="@style.css">
               <HBox.margin>
                  <Insets bottom="30.0" top="30.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextArea>
            <TextArea fx:id="password" prefHeight="41.0" prefWidth="161.0" promptText="password" styleClass="jfx-text-field" stylesheets="@style.css">
               <HBox.margin>
                  <Insets bottom="30.0" top="30.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </TextArea>
              <Button fx:id="editUser" mnemonicParsing="false" onMousePressed="#edit" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: FED700;" styleClass="recipeAnchorStyle" stylesheets="@style.css" text="Edit">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
              <Button fx:id="removeUser" mnemonicParsing="false" onMousePressed="#remove" prefHeight="32.0" prefWidth="64.0" style="-fx-background-color: FED700;" styleClass="recipeAnchorStyle" stylesheets="@style.css" text="Remove">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
         </children>
      </HBox>
    </children>
    <styleClass>
        <String fx:value="recipeAnchorStyle" />
        <String fx:value="transparent" />
    </styleClass>
</AnchorPane>
